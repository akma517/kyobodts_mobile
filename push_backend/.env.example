# Firebase 서비스 계정 키 설정 (둘 중 하나 선택)

# 방법 1: 서비스 계정 키 파일 경로
FIREBASE_SERVICE_ACCOUNT_PATH=/path/to/serviceAccountKey.json

# 방법 2: 서비스 계정 키 JSON 직접 설정
# FIREBASE_SERVICE_ACCOUNT_JSON={"type":"service_account","project_id":"kyobodts-mobile",...}

# 서버 설정
FLASK_ENV=development
FLASK_DEBUG=True
PORT=5000